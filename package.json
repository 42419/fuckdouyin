{
  "name": "douyin-downloader",
  "version": "1.0.0",
  "description": "抖音视频下载工具",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev:frontend": "node server.js",
    "dev:backend": "npm run dev --prefix backend-hono",
    "dev": "concurrently -k -n \"FRONT,BACK\" -c \"blue,green\" \"node server.js\" \"cd backend-hono && node node_modules/wrangler/bin/wrangler.js dev src/index.ts\"",
    "build": "node tools/hash-build.js"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.2"
  },
  "author": "",
  "license": "MIT"
}
